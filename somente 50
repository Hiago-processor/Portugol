programa {
  funcao inicio() {
  
  inteiro numero, c, maiorquecinquenta = 0
  
  para( c = 1; c <= 4; c++){
    escreva("Digite um numero: ")
    leia(numero)
  
    se(numero >= 50)
      maiorquecinquenta++
  }
  escreva("Quantidade de numeros lidos maiores que cinquenta: ", maiorquecinquenta)
  }
  
}
