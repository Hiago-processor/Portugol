programa {
 inteiro bolinhas, i
 funcao inicio() {
   para(i = 1; i <= 5; i++){
     escreva("Digite o número de bolinhas de gude que você acha que tem dentro do pote: \n")
     leia(bolinhas)

    se (bolinhas == 82){
     escreva("Parabéns, você acertou\n")pare}

    senao se(bolinhas < 82)
     escreva("Você errou! Existem mais bolinhas do que você digitou\n")

    senao se(bolinhas > 82)
     escreva("Você errou! Existem menos bolinhas do que você digitou\n")
    
  
   }
  
 }
}
