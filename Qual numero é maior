programa {
inteiro num1, num2, maior
funcao inicio() {
  escreva("Digite um número: ")
  leia(num1)
  escreva("Digite outro número: ")
  leia(num2)

  se (num1 > num2){
   escreva("O maior número é ", num1)
  }
  senao se(num2 > num1){
   escreva("O maior número é ", num2)
  }
  senao{
   escreva("Os números são iguais")
  }

}
}
